
<h1>Articles</h1>

<a href="/articles/new">Create article</a>

<h1>Listing articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>ID</th>
  </tr>

  <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.text %></td>
        <td><%= article.id %></td>
      </tr>
  <% end %>
</table>
